{
  "name": "@das-buro-am-draht/lambda-pdf-core",
  "version": "1.1.3",
  "publishConfig": {
    "registry": "https://github.com/das-buro-am-draht/lambda-pdf-core"
  },
  "files": [
    "src",
    "index.js"
  ],
  "description": "Lambda PDF Core package",
  "main": "index.js",
  "scripts": {
    "test": "jest",
    "compile": "ncc build ./src/handler.js -o compiled -e puppeteer-core -e chrome-aws-lambda -e puppeteer",
    "compile-min": "npm run compile -- --minify",
    "compile-obfuscated": "npm run compile-min && npm run obfuscate",
    "obfuscate": "javascript-obfuscator ./compiled/index.js --output ./compiled/lambda-pdf-core.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/das-buro-am-draht/lambda-pdf-core.git"
  },
  "keywords": [
    "lambda",
    "pdf",
    "core",
    "puppeteer"
  ],
  "author": "Franz Lacherbauer",
  "license": "ISC",
  "dependencies": {
    "@pdf-lib/fontkit": "0.0.4",
    "ajv": "^6.12.6",
    "atob": "^2.1.2",
    "axios": "^0.21.1",
    "chrome-aws-lambda": "^8.0.0",
    "handlebars": "^4.7.7",
    "pdf-lib": "^1.16.0",
    "puppeteer": "^8.0.0",
    "puppeteer-core": "^8.0.0",
    "require-directory": "^2.1.1",
    "serverless": "^1.83.2",
    "serverless-offline": "^5.12.0",
    "serverless-plugin-warmup": "^4.9.0"
  },
  "devDependencies": {
    "@vercel/ncc": "^0.28.2",
    "javascript-obfuscator": "^2.11.1",
    "jest": "^26.6.3"
  },
  "jest": {
    "setupFilesAfterEnv": [
      "<rootDir>/jest/jestSetup.js"
    ],
    "collectCoverage": true,
    "verbose": true
  }
}
